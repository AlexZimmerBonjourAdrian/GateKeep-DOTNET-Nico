# Archivo de ejemplo de variables de Terraform
# Copia este archivo a terraform.tfvars y ajusta los valores según tu configuración
# NOTA: terraform.tfvars está en .gitignore para no subir credenciales

# Región de AWS
aws_region = "sa-east-1"

# Ambiente
environment = "dev"

# Nombre del proyecto
project_name = "gatekeep"

# Perfil de AWS (si tienes múltiples perfiles en ~/.aws/credentials)
# Si tienes un perfil llamado "Alex", úsalo así:
aws_profile = "Alex"

# Si prefieres usar variables de entorno en lugar de perfil, deja esto vacío:
# aws_profile = ""

