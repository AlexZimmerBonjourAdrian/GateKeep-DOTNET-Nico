{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "EC2FullAccess",
			"Effect": "Allow",
			"Action": [
				"ec2:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "ECSFullAccess",
			"Effect": "Allow",
			"Action": [
				"ecs:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "ECRFullAccess",
			"Effect": "Allow",
			"Action": [
				"ecr:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "RDSFullAccess",
			"Effect": "Allow",
			"Action": [
				"rds:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "ElastiCacheFullAccess",
			"Effect": "Allow",
			"Action": [
				"elasticache:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "AmazonMQFullAccess",
			"Effect": "Allow",
			"Action": [
				"mq:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "SecretsManagerFullAccess",
			"Effect": "Allow",
			"Action": [
				"secretsmanager:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "SSMFullAccess",
			"Effect": "Allow",
			"Action": [
				"ssm:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "CloudWatchFullAccess",
			"Effect": "Allow",
			"Action": [
				"cloudwatch:*",
				"logs:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "Route53FullAccess",
			"Effect": "Allow",
			"Action": [
				"route53:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "S3FullAccess",
			"Effect": "Allow",
			"Action": [
				"s3:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "CloudFrontFullAccess",
			"Effect": "Allow",
			"Action": [
				"cloudfront:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "ACMCertificateFullAccess",
			"Effect": "Allow",
			"Action": [
				"acm:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "IAMFullAccess",
			"Effect": "Allow",
			"Action": [
				"iam:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "ELBFullAccess",
			"Effect": "Allow",
			"Action": [
				"elasticloadbalancing:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "AppRunnerFullAccess",
			"Effect": "Allow",
			"Action": [
				"apprunner:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "STSAssumeRole",
			"Effect": "Allow",
			"Action": [
				"sts:GetCallerIdentity",
				"sts:AssumeRole"
			],
			"Resource": "*"
		},
		{
			"Sid": "TaggingAccess",
			"Effect": "Allow",
			"Action": [
				"tag:GetResources",
				"tag:TagResources",
				"tag:UntagResources"
			],
			"Resource": "*"
		},
		{
			"Sid": "AllowIAMUserChangePassword",
			"Effect": "Allow",
			"Action": [
				"iam:ChangePassword",
				"iam:GetUser"
			],
			"Resource": "arn:aws:iam::126588786097:user/Alex"
		}
	]
}
